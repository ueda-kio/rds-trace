{
	"name": "app",
	"private": true,
	"version": "0.0.1",
	"scripts": {
		"dev": "pnpm turbo autoCreate && pnpm dev:serve",
		"dev:serve": "FORCE_COLOR=true next dev -p 23000 | pino-pretty",
		"build": "next build --no-lint",
		"start": "next start",
		"check": "biome check",
		"check:fix": "biome check --write",
		"lint:style": "stylelint --ignore-path .gitignore './src/**/*.css'",
		"tc": "tsc --noEmit",
		"test": "jest --silent --coverage",
		"test:watch": "pnpm turbo autoCreate && jest --watch",
		"sb": "pnpm turbo autoCreate && storybook dev -p 26006",
		"new:fc": "hygen new fc",
		"new:page": "hygen new page",
		"new:local": "hygen new local",
		"autoCreate": "run-p autoCreate:*",
		"autoCreate:zodios-be-internal": "openapi-zod-client --additional-props-default-value=false --no-with-alias src/lib/server/api/spec/openapi.yml --output src/lib/server/api/dist/internalApiClient.ts",
		"autoCreate:pathpida": "pathpida --output src/lib --enableStatic",
		"postinstall": "pnpm turbo autoCreate",
		"e2e": "playwright test",
		"e2e:ui": "playwright test --ui"
	},
	"dependencies": {
		"@tanstack/react-query": "4.36.1",
		"@trpc/client": "10.45.2",
		"@trpc/next": "10.45.2",
		"@trpc/react-query": "10.45.2",
		"@trpc/server": "10.45.2",
		"@zodios/core": "10.9.6",
		"axios": "1.7.7",
		"clsx": "2.1.1",
		"http-status-codes": "2.3.0",
		"ioredis": "5.4.1",
		"msw": "1.3.5",
		"nanoid": "4.0.2",
		"nanoid-dictionary": "4.3.0",
		"next": "14.2.15",
		"next-session": "4.0.5",
		"openid-client": "5.7.0",
		"query-string": "8.2.0",
		"react": "18.3.1",
		"react-dom": "18.3.1",
		"zod": "3.23.8"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@storybook/addon-a11y": "7.4.6",
		"@storybook/addon-actions": "7.4.6",
		"@storybook/addon-essentials": "7.4.6",
		"@storybook/nextjs": "7.4.6",
		"@storybook/react": "7.4.6",
		"@storybook/testing-library": "0.2.2",
		"@storybook/testing-react": "2.0.1",
		"@testing-library/jest-dom": "6.6.2",
		"@testing-library/react": "16.0.1",
		"@testing-library/user-event": "14.5.2",
		"@types/node": "18.19.31",
		"@types/react": "18.3.12",
		"@types/react-dom": "18.3.1",
		"husky": "9.1.6",
		"hygen": "6.2.11",
		"jest-environment-jsdom": "29.7.0",
		"jest-html-reporters": "3.1.7",
		"msw-storybook-addon": "1.10.0",
		"next-router-mock": "0.9.13",
		"node-mocks-http": "1.15.1",
		"npm-run-all": "4.1.5",
		"openapi-zod-client": "1.18.2",
		"pathpida": "0.22.0",
		"pino-pretty": "13.0.0",
		"storybook": "7.4.6",
		"storycap": "5.0.1",
		"stylelint": "16.9.0",
		"ts-loader": "9.5.1",
		"ts-node": "10.9.2",
		"whatwg-fetch": "3.6.20"
	},
	"engines": {
		"node": "18.20.2",
		"pnpm": "9.6.0"
	},
	"msw": {
		"workerDirectory": "public"
	}
}
